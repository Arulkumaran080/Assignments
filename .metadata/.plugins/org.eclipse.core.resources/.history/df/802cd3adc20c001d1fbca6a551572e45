package Strings;

public class str {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String t="Delhi";
		String o="Mumbai";
		String k="delhi";
		String y=new String("Mumbai");
		String l=new String("Delhi");
		String p=new String("Hello");
		if(o.equals(l)&&o==l)
		{
			System.out.println("Yes");
		}
		else
		{
			System.out.println("No");
		}
		if(y.equals(p)&&y==p)
		{
			System.out.println("Yes");
		}
		else
		{
			System.out.println("No");
		}
		if(t.equals(o)&&t==o)
		{
			System.out.println("Yes");
		}
		else
		{
			System.out.println("No");
		}
		if(k.equals(y)&&k==y)
		{
			System.out.println("Yes");
		}
		else
		{
			System.out.println("No");
		}
		if(p.equals(y)&&p==y)
		{
			System.out.println("Yes");
		}
		else
		{
			System.out.println("No");
		}
	}

}
